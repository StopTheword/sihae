includes:
  - vendor/phpstan/phpstan-doctrine/extension.neon
  - vendor/phpstan/phpstan-deprecation-rules/rules.neon
  - vendor/phpstan/phpstan-strict-rules/rules.neon

parameters:
  level: max
  ignoreErrors:
    - '#Method [A-z]+?::[A-z]+?\(\) should return [A-z]+?\|null but returns object\|null\.#'
    - '#Dynamic call to static method PHPUnit\\Framework\\Assert::[A-z]+\(\).#'
